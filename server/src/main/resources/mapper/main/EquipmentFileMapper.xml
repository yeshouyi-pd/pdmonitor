    <if test="_parameter != null">
      <include refid="Example_Where_Clause" />
    </if>
    and   TO_DAYS(cjsj) = TO_DAYS(NOW())  and  tplj LIKE '%.png'  GROUP  BY  deptcode

  </select>


  <select id="getthisDeptjxsjJT" parameterType="com.pd.server.main.domain.EquipmentFileExample" resultMap="BaseResultMap">
    SELECT  b. * , a. sbmc   FROM  water_equipment a, (
    select * from equipment_file
    <if test="_parameter != null">
      <include refid="Example_Where_Clause" />
    </if>
    AND  TO_DAYS(cjsj) = TO_DAYS(NOW()) and  tplj LIKE '%.png'   ) b WHERE a.sbsn = b.sbbh ORDER BY b.sbbh  , b.cjsj DESC
  </select>

